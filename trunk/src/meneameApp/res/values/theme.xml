<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="Theme.mnm" parent="android:Theme">
		<item name="android:windowBackground">@color/Color.orange</item>
        <item name="android:windowNoTitle">true</item>
	</style>
	<style name="Style.infolabel" parent="android:Theme">
	    <item name="android:textSize">18sp</item>
	    <item name="android:textColor">#FFF</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="Style.infolabel_orange" parent="android:Theme">
	    <item name="android:textSize">18sp</item>
	    <item name="android:textColor">@color/Color.orange</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="Style.app_logo" parent="android:Theme">
	    <item name="android:background">@drawable/logo</item>
	</style>
	<style name="Style.orange_bg" parent="android:Theme">
	    <item name="android:background">@color/Color.orange</item>
	</style>
	<style name="Style.grey_bg" parent="android:Theme">
	    <item name="android:background">@color/Color.grey</item>
	</style>
	<style name="Style.white_bg" parent="android:Theme">
	    <item name="android:background">@color/Color.white</item>
	</style>
	<color name="Color.grey">#CCC</color>
	<color name="Color.white">#EEE</color>
	<color name="Color.black">#000</color>
	<color name="Color.orange">#FF6400</color>
	<color name="Color.dark_blue">#202C8A</color>
</resources>
